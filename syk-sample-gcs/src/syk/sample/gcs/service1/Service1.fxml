<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="syk.sample.gcs.service1.Service1Controller">
    <bottom>
        <HBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #b0d7dc;" BorderPane.alignment="CENTER">
            <children>
                <!-- fX에 태그타입 아이디로 필드 선언해주기 EX) @FXML private Button btnOK; -->
                <Button fx:id="btnOK" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" text="OK" />
                <Button fx:id="btnCancel" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" text="Cancel" />
            </children>
        </HBox>
    </bottom>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="68.0" layoutY="68.0" text="위도" />
            <Label layoutX="68.0" layoutY="115.0" text="경도" />
            <TextField fx:id="txtLat" layoutX="113.0" layoutY="64.0" />
            <TextField fx:id="txtLng" layoutX="113.0" layoutY="111.0" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
